uid = swift
gid = swift
log file = /var/log/rsyncd.log
pid file = /var/run/rsyncd.pid
address = OBJECT3_IP_PRIVATE_CHANGEME

[account]
max connections = 2
path = /srv/node/swift-devices/
read only = False
lock file = /var/lock/account.lock

[container]
max connections = 2
path = /srv/node/swift-devices/
read only = False
lock file = /var/lock/container.lock

[object]
max connections = 2
path = /srv/node/swift-devices/
read only = False
lock file = /var/lock/object.lock
